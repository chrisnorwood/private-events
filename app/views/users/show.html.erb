<h3><%= @user.name %>'s Events:</h3>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th>Time</th>
  </tr>
  <% if @user.created_events.any?
      @user.created_events.each do |event| %>
        <tr>
          <td><%= link_to event.title, event_url(event)%></td>
          <td><%= event.description %></td>
          <td><%= event.location %></td>
          <td><%= event.time %></td>
        </tr>
  <%  end
    else %>
    <tr>
      <td>No events found!</td>
    </tr>
  <% end %>
</table>